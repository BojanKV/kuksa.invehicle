[Unit]
Description=ofono fix for kuksa dongle internet service             
After=ofono.service
  
[Service]
Type=simple
WorkingDirectory=/usr/bin/kuksa-dongle
ExecStart=/usr/bin/python3 /usr/bin/kuksa-dongle/ofono-fix.py
StandardError=null            
                              
[Install]                     
WantedBy=multi-user.target

